    "# Product of Array Except Self\n",
    "#do not use division, O(n)\n",
    "nums=[1,2,3,4,5]\n",
    "p=1\n",
    "a=[]\n",
    "for i in range(len(nums)):\n",
    "    a.append(p)\n",
    "    p=p*nums[i]\n",
    "print(a)\n",
    "q=1\n",
    "for j in range(len(nums)-1,-1,-1):\n",
    "    a[j]=a[j]*q\n",
    "    q=q*nums[j]\n",
    "a"
   ]
